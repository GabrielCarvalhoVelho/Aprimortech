rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /clientes/{clienteId} {
      allow read, write: if request.auth != null;
    }
    match /contatos/{contatoId} {
      allow read, write: if request.auth != null;
    }
    match /setores/{setorId} {
      allow read, write: if request.auth != null;
    }
    match /maquinas/{maquinaId} {
      allow read, write: if request.auth != null;
    }
    match /pecas/{pecaId} {
      allow read, write: if request.auth != null;
    }
    match /relatorios/{relatorioId} {
      allow read, write: if request.auth != null;
    }
    match /defeitos/{defeitoId} {
      allow read, write: if request.auth != null;
    }
    match /servicos/{servicoId} {
      allow read, write: if request.auth != null;
    }
    match /tintas/{tintaId} {
      allow read, write: if request.auth != null;
    }
    match /solventes/{solventeId} {
      allow read, write: if request.auth != null;
    }
  }
}

